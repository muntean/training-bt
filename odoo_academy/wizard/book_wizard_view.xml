<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="library_book_wizard_view">
            <field name="name">library.book.wizard.view</field>
            <field name="model">library.book.wizard</field>
            <field name="arch" type="xml">
                <form string="Books of Customer">
                    <group>
                        <field name="partner_id"/>
                    </group>
                    <group>
                        <field name="rental_ids"/>
                    </group>
                    <footer>
                        <button name="action_search" type="object" string="Search" class="oe_highlight"/>
                        or
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>